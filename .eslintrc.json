{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
         "import/no-webpack-loader-syntax": "off",
         "react/jsx-uses-react": "error",   
     "react/jsx-uses-vars": "error" ,
     "no-unused-vars": 0,
       "react/prop-types": "off"
    }
}
